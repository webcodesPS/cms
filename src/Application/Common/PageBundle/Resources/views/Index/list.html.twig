{% extends 'ApplicationCommonPageBundle::base.html.twig' %}

{% block body %}

    {{ dump(page) }}

    {% for key, value in gallery %}
        <img src="{% path value.media.id, 'small' %}">
    {% endfor %}

    {% if page[0].translates[0] is defined %}
        {{ page[0].translates[0].translate }}
    {% endif %}

{% endblock %}